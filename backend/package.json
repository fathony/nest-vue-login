{
  "name": "my-protokol",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "typeorm": "env-cmd ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:generate": "npm run typeorm -- --dataSource=src/database/data-source.ts migration:generate",
    "migration:create": "npm run typeorm -- migration:create",
    "migration:run": "npm run typeorm -- --dataSource=src/database/data-source.ts migration:run",
    "migration:revert": "npm run typeorm -- --dataSource=src/database/data-source.ts migration:revert",
    "schema:drop": "npm run typeorm -- --dataSource=src/database/data-source.ts schema:drop",
    "seed:run": "ts-node -r tsconfig-paths/register ./src/database/seeds/run-seed.ts",
    "console": "ts-node -r tsconfig-paths/register ./src/console/run-console.ts",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "start:cron": "node dist/cron-app.js",
    "start:cron-dev": "ts-node -r dotenv/config src/main.cron.ts",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "env-cmd jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/axios": "^0.1.0",
    "@nestjs/common": "9.1.6",
    "@nestjs/config": "2.2.0",
    "@nestjs/core": "9.1.6",
    "@nestjs/event-emitter": "^1.4.1",
    "@nestjs/jwt": "9.0.0",
    "@nestjs/passport": "9.0.0",
    "@nestjs/platform-express": "9.1.6",
    "@nestjs/platform-socket.io": "9.1.6",
    "@nestjs/schedule": "^2.2.1",
    "@nestjs/serve-static": "^3.0.1",
    "@nestjs/swagger": "^7.1.6",
    "@nestjs/typeorm": "^10.0.0",
    "@nestjs/websockets": "9.1.6",
    "axios": "^0.27.2",
    "bcryptjs": "2.4.3",
    "class-transformer": "0.5.1",
    "class-validator": "^0.14.0",
    "cron": "^2.4.0",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.0.3",
    "handlebars": "^4.7.8",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "multer": "1.4.4",
    "nestjs-typeorm-paginate": "^4.0.4",
    "nodemailer": "^6.9.5",
    "passport": "0.6.0",
    "passport-anonymous": "1.0.1",
    "passport-jwt": "4.0.0",
    "pg": "^8.11.2",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.5.7",
    "socket.io": "^4.6.1",
    "source-map-support": "0.5.21",
    "swagger-ui-express": "^5.0.0",
    "typeorm": "^0.3.17",
    "uuid": "^9.0.0",
    "winston": "^3.10.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@nestjs/cli": "9.1.5",
    "@nestjs/schematics": "9.0.3",
    "@nestjs/testing": "9.1.6",
    "@types/bcryptjs": "2.4.2",
    "@types/cron": "^2.4.0",
    "@types/express": "4.17.14",
    "@types/jest": "29.2.3",
    "@types/multer": "1.4.7",
    "@types/node": "16.18.3",
    "@types/nodemailer": "^6.4.9",
    "@types/passport-anonymous": "1.0.3",
    "@types/passport-jwt": "3.0.7",
    "@types/supertest": "2.0.12",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "5.43.0",
    "@typescript-eslint/parser": "5.43.0",
    "env-cmd": "10.1.0",
    "eslint": "8.27.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "7.0.4",
    "is-ci": "3.0.1",
    "jest": "29.3.1",
    "prettier": "2.7.1",
    "supertest": "6.3.1",
    "ts-jest": "29.0.1",
    "ts-loader": "9.4.1",
    "ts-node": "10.9.1",
    "tsconfig-paths": "^4.1.0",
    "tslib": "2.4.1",
    "typescript": "4.8.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
